BEGIN begin
ID matches
EQ =
FIND find
REGEX '([A-Z a-z])*ment([A-Z a-z])*'
ID in
ASCII-STR "input1.txt"
INTERS inters
FIND find
REGEX '(A|a) ([A-Z a-z])*'
ID in
ASCII-STR "input2.txt"
SEMICOLON ;
ID n_matches
EQ =
HASH #
ID matches
SEMICOLON ;
PRINT print
OPENPARENS (
ID n_matches
CLOSEPARENS )
SEMICOLON ;
ID replace
REGEX '([A-Z a-z])*ment'
ID with
ASCII-STR ""
ID in
ASCII-STR "input1.txt"
GRTNOT >!
ASCII-STR "output1.txt"
SEMICOLON ;
ID end
